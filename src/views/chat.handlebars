<br>
<h1 class="divHeader">Chat</h1>
<br>
<div>
  <form action="" class="form">
    <ul>
      <li>
        <i id="user" class="user"></i>
        <input id="chatbox" class="input" />
      </li>
    </ul>
  </form>

</div>

<div id="logs" class="divHeader">
  {{#each messages}}
    <div class="message divHeader">
      <span class="user">{{this.user}}</span>:
      {{this.message}}
    </div>
  {{/each}}
</div>

