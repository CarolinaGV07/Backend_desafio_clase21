<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Profile</title>
</head>

<body>
  <header>
    <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
      <a class="navbar-brand" href="#">E-commerce</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
        aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item">
            <button class="nav-link btn btn-link" onclick="location.href='/'">Home</button>
          </li>
          <li class="nav-item">
            <button class="nav-link btn btn-link" onclick="location.href='/api/products'">Products</button>
          </li>
          <li class="nav-item">
            <button class="nav-link btn btn-link" onclick="location.href='/api/users/premium/{{_id}}'">Change to
              rol</button>
          </li>
          <li class="nav-item">
        <button class="nav-link btn btn-link" onclick="location.href='/api/session/logout'">Logout</button>
      </li>
        </ul>
      </div>
    </nav>
  </header>
  <br>
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-md-6">
        <h1 style="color:rgb(186, 72, 195)">Profile</h1>
        <br>
        <div class="container">
          <p>First Name: {{first_name}}</p>
          <p>Last Name: {{last_name}}</p>
          <p>Age: {{age}}</p>
          <p>Email: {{email}}</p>
          <p>Rol: {{rol}}</p>
        </div>
      </div>
    </div>
  </div>

</body>

</html>